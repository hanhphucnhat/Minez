                <Button x:Name="Buuton_VitTai_NO" Content="Vít tải" HorizontalAlignment="Center" VerticalAlignment="Center" IsTabStop="False" Width="67" Height="40" Margin="5" Background="#3F00FF00" Foreground="White" BorderBrush="White" Click="Buuton_VitTai_NO_Click" BorderThickness="1" FontSize="11"/>
                <Button x:Name="Button_BangTai_NO" Content="Băng tải" HorizontalAlignment="Center" VerticalAlignment="Center" IsTabStop="False" Width="67" Height="40" Margin="5" Background="#3F00FF00" Foreground="White" BorderBrush="White" Click="Button_BangTai_NO_Click" BorderThickness="1" FontSize="11" />
