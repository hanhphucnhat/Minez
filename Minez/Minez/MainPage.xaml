                    <TextBlock Name="Lbl_Time" HorizontalAlignment="Center" FontSize="24" Foreground="#efff" Margin="0,20,0,0">10:18 AM</TextBlock>
                    <TextBlock Name="Lbl_Date" Grid.Row="1" HorizontalAlignment="Center" Foreground="#efff" Margin="0,5,0,5" FontSize="20">September 4, 2015</TextBlock>
                </StackPanel>
                <TextBlock x:Name="MinezAutomatic" Height="80" Margin="5,20,10,0" TextWrapping="Wrap" Text="Minez Automatic" VerticalAlignment="Center" Width="400" FontFamily="UVN Thoi Dai" FontSize="48" HorizontalAlignment="Center" FontWeight="Bold" TextAlignment="Right"/>


