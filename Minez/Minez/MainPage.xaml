                <TextBox x:Name="textDoAmHienTai" HorizontalAlignment="Left" Height="30" Margin="643,50,-297.667,0" TextWrapping="Wrap" Text="17" Width="64" TextAlignment="Right"/>
                <TextBlock x:Name="textBlock_Copy9" HorizontalAlignment="Left" Height="25" Margin="707,15,-296.667,1.333" TextWrapping="Wrap" Text="%" VerticalAlignment="Center" Width="25" Foreground="#FF0066FF" TextAlignment="Center"/>
