            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0" Width="775" Height="50">
                <Button x:Name="Button_BangTay" Content="Bằng Tay" HorizontalAlignment="Center" VerticalAlignment="Center" IsTabStop="False" Width="67" Height="40" Margin="5" Background="#3F0000FF" Foreground="White" BorderBrush="White" Click="Button_BangTay_Click" BorderThickness="1" FontSize="11"/>
                <Button x:Name="Button_VaoLo" Content="Vào Lò" HorizontalAlignment="Center" VerticalAlignment="Center" IsTabStop="False" Width="67" Height="40" Margin="5" Background="#3F0000FF" Foreground="White" BorderBrush="White" Click="Button_VaoLo_Click" BorderThickness="1" FontSize="11"/>
                <Button x:Name="Button_RaLo" Content="Ra Lò" HorizontalAlignment="Center" VerticalAlignment="Center" IsTabStop="False" Width="67" Height="40" Margin="5" Background="#3F0000FF" Foreground="White" BorderBrush="White" Click="Button_RaLo_Click" BorderThickness="1" FontSize="11"/>
