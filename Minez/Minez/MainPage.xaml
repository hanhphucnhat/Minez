            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0" Width="800" Height="55">
                <Button x:Name="button1" HorizontalAlignment="Left" Height="48" Margin="5,0" Width="48" Tapped="button1_Tapped">
                    <Image x:Name="image" HorizontalAlignment="Left" Height="48" Margin="-10,-6" VerticalAlignment="Top" Width="48" Source="Resources/Images/Home.png"/>
                </Button>
                <Button x:Name="button" HorizontalAlignment="Left" Height="48" Margin="5,0" Width="48" Tapped="button_Tapped">
                    <Image x:Name="image1" HorizontalAlignment="Left" Height="48" Margin="-10,-6" VerticalAlignment="Top" Width="48" Source="Resources/Images/Settings.png"/>
                </Button>
