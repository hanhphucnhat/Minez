                <Button x:Name="Button_QuatGau_NC" Content="Quạt Gầu" HorizontalAlignment="Center" VerticalAlignment="Center" IsTabStop="False" Width="67" Height="40" Margin="5" Background="Red" Foreground="White" BorderBrush="White" Click="Button_QuatGau_NC_Click" BorderThickness="1" FontSize="11"/>
                <Button x:Name="Button_CapLieu_NC" Content="Cấp Liệu" HorizontalAlignment="Center" VerticalAlignment="Center" IsTabStop="False" Width="67" Height="40" Margin="5" Background="Red" Foreground="White" BorderBrush="White" Click="Button_CapLieu_NC_Click" BorderThickness="1" FontSize="11"/>
                <Button x:Name="Button_QuatLo_NC" Content="Quạt Lò" HorizontalAlignment="Center" VerticalAlignment="Center" IsTabStop="False" Width="67" Height="40" Margin="5" Background="Red" Foreground="White" BorderBrush="White" Click="Button_QuatLo_NC_Click" BorderThickness="1" FontSize="11"/>
                <Button x:Name="Button_GauBe_NC" Content="Gầu Bể" HorizontalAlignment="Center" VerticalAlignment="Center" IsTabStop="False" Width="67" Height="40" Margin="5" Background="Red" Foreground="White" BorderBrush="White" Click="Button_GauBe_NC_Click" BorderThickness="1" FontSize="11"/>
