                <TextBox x:Name="textThoiGianCapLieu" HorizontalAlignment="Left" Height="30" Margin="183,10,-13.667,0" TextWrapping="Wrap" Text="5" Width="64" TextAlignment="Right"/>
                <TextBox x:Name="textThoiGianDung" HorizontalAlignment="Left" Height="30" Margin="183,50,-13.667,0" TextWrapping="Wrap" Text="10" Width="64" TextAlignment="Right"/>
                <TextBlock x:Name="textBlock_Copy1" HorizontalAlignment="Left" Height="25" Margin="247,15,-12.667,1.333" TextWrapping="Wrap" Text="s" VerticalAlignment="Center" Width="25" Foreground="#FF0066FF" TextAlignment="Center"/>
                <TextBlock x:Name="textBlock_Copy2" HorizontalAlignment="Left" Height="25" Margin="247,55,-12.667,1.333" TextWrapping="Wrap" Text="s" VerticalAlignment="Center" Width="25" Foreground="#FF0066FF" TextAlignment="Center"/>

                <TextBlock x:Name="textBlock_Copy3" HorizontalAlignment="Left" Height="30" Margin="286,15,-65.667,0.333" TextWrapping="Wrap" Text="Nhiệt độ cài đặt:" VerticalAlignment="Center" Width="125" Foreground="#FF0066FF"/>
